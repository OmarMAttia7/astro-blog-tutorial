---
import Hamburger from "./Hamburger.astro";
import Navigation from "./Navigation.astro";
import ThemeIcon from "./ThemeIcon.astro";

interface Props {
  activeUrl: string;
}

const { activeUrl } = Astro.props;
---

<header>
  <nav>
    <Hamburger />
    <ThemeIcon />
    <Navigation {activeUrl} />
  </nav>
</header>
